{
  "name": "node_skeleton",
  "version": "0.0.1",
  "private": true,
  "dependencies": {
    "body-parser": "1.15.2",
    "chalk": "2.0.1",
    "colors": "1.1.2",
    "cookie-parser": "1.4.3",
    "debug": "2.2.0",
    "dotenv": "4.0.0",
    "express": "4.14.1",
    "express-api-doc": "0.0.11",
    "express-flash": "0.0.2",
    "express-session": "1.15.3",
    "express-validator": "3.2.0",
    "express-winston": "2.4.0",
    "lodash": "4.17.4",
    "memory-cache": "0.1.6",
    "moment": "2.18.1",
    "multer": "1.3.0",
    "nodemailer": "4.0.1",
    "nodemon": "1.11.0",
    "promise-mysql": "3.1.3",
    "raven": "2.1.0",
    "underscore": "^1.8.3",
    "uuid": "3.1.0",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "apidoc": "0.17.6",
    "chai": "4.1.0",
    "eslint": "3.19.0",
    "eslint-config-airbnb-base": "11.2.0",
    "eslint-plugin-import": "2.7.0",
    "istanbul": "0.4.5",
    "jshint": "2.9.4",
    "maildev": "1.0.0-rc3",
    "mocha": "3.2.0",
    "supertest": "3.0.0"
  },
  "scripts": {
    "start": "nodemon ./bin/www",
    "watch": "node_modules/.bin/mocha -w --reporter nyan",
    "cov": "istanbul cover node_modules/.bin/_mocha  -R test/**/*",
    "lint": "eslint **/*.js",
    "lintfix": "eslint $FILE --fix",
    "testunit": "node_modules/.bin/mocha --reporter nyan test/testsuiteunit.js",
    "testapi": "node_modules/.bin/mocha --reporter nyan test/testsuiteapi.js",
    "apidocs": "apidoc -i ./routes -o public/api",
    "ie-dev": "cp -f env-development.env .env",
    "ie-staging": "cp -f env-staging.env .env"
  }
}
